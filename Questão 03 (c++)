#include <iostream>

using namespace std;

void pnz()
{
    float c;
    cin>>c;
    if(c>0){
        cout<<"1";
    }else{
        if(c<0){
            cout<<"-1";
        }else{
            if(c==0){
                cout<<"0";
            }
        }
    }
}

int main()
{
    pnz();

    return 0;
}
